{
  "title": "Fraud Detection API Metrics",
  "tags": ["fraud-detection", "mlops", "api"],
  "timezone": "browser",
  "editable": true,
  "refresh": "5s",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "id": 1,
      "title": "Request Rate (requests/sec)",
      "type": "timeseries",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(http_requests_total{job=\"fraud-detection-api\"}[5m])",
          "legendFormat": "{{method}} {{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 2,
      "title": "Prediction Latency (seconds)",
      "type": "timeseries",
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(prediction_duration_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, rate(prediction_duration_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Fraud Detection Rate",
      "type": "stat",
      "gridPos": {"x": 0, "y": 8, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "sum(rate(fraud_predictions_total{prediction=\"fraud\"}[5m])) / sum(rate(fraud_predictions_total[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 0.3, "color": "yellow"},
              {"value": 0.6, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Total Predictions",
      "type": "stat",
      "gridPos": {"x": 6, "y": 8, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "sum(fraud_predictions_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "blue"}
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "API Error Rate",
      "type": "timeseries",
      "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(http_requests_total{job=\"fraud-detection-api\",status=~\"5..\"}[5m])",
          "legendFormat": "5xx errors",
          "refId": "A"
        },
        {
          "expr": "rate(http_requests_total{job=\"fraud-detection-api\",status=~\"4..\"}[5m])",
          "legendFormat": "4xx errors",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Feature Engineering Latency",
      "type": "timeseries",
      "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(feature_engineering_duration_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, rate(feature_engineering_duration_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 7,
      "title": "Risk Level Distribution",
      "type": "piechart",
      "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "sum by (risk_level) (fraud_predictions_total)",
          "legendFormat": "{{risk_level}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      }
    }
  ]
}
